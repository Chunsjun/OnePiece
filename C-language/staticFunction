#include <stdio.h>
#include <string.h>

char* Reverse(const char *str);

int main()
{
char s1[] = "abc";
char *res = NULL;

res = Reverse(s1);
printf("res = %s\n", res);

return 0;
}

char* Reverse(const char *str)
{
static char result[80];
int i;
int len = strlen(str);

for(i=0; i<len; i++)
result[i] = str[len - i - 1];
result[i] = '\0';

return result;
}
