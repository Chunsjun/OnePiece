#include <stdio.h>

int main()
{
/*파일생성*/
FILE *fp = fopen("text1.txt", "wt");
fputs("123456789", fp);
fclose(fp);

/*파일개방*/
fp=fopen("text1.txt", "rt");

/*SEEK_END test1 */
fseek(fp, -2, SEEK_END);
putchar(fgetc(fp));

/*SEEK_SET test1 */
fseek(fp, 2, SEEK_SET);
putchar(fgetc(fp));

/*SEEK_CUR test1 */
fseek(fp, 2, SEEK_CUR);
putchar(fgetc(fp));

fclose(fp);
return 0;
}
  
