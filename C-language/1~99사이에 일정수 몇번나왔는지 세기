#include <stdio.h>  
#include <stdlib.h>
int main(void)    
{
    int a[100];
    int b[10];
    int i;
    int t=0;
     
    for(i=0; i<10; i++) b[i]=0;
     
    for(i=0; i<10; i++){
        scanf("%d",&a[i]);
        if(a[i]==0) break;
        t=a[i]/10;
        b[t]++;        
        } 
     
    for(i=0; i<10; i++){
         if(b[i]>0)
         printf("\n%d : %dê°œ",i,b[i]); 
         }
         
    system("pause");
    return 0;
    
}
