#include <stdio.h>
#include <stdlib.h>
 
int main(void)
{
    int i, m, horsu = 0, jjacsu = 0;
 
 
 
    for (i = 0; i < 10; i++) {
        scanf_s("%d", &m);
        if (m % 2 == 0) {
            jjacsu++;
        }
        else if (m% 2 == 1) {
 
            horsu++;
        }
    }
    printf("짝수 : %d \n", jjacsu);
    printf("홀수 : %d \n", horsu);
 
    system("pause");
    return 0;
}
