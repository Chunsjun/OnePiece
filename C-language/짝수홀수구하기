#include <stdio.h>
#include <stdlib.h>
int main(void)
{
    int cnt=0,num,s=0,o=0;
     
    while(cnt<10)
    {
        cnt=cnt+1;
        scanf("%d",&num); 
         
        if(num%3==0) s++;
        if(num%5==0) o++;
    }//while
     
    printf("3의 배수 : %d개\n",s);
    printf("5의 배수 : %d개\n",o); 
     
     
 
    system("pause");
    return 0;
}
