public class javalang {  public static void main(String[] args)   {   char[] alphabets = new char[26];   for (int i=0; i<alphabets.length; i++)     alphabets[i] = (char)('\u0041' + i);       int[] count = new int[26];   for(int i=0; i<count.length; i++)    count[i] = 0;      String str = "I heard that you're settled down That you found a girl and you're married now.";   str = str.toUpperCase();      for(int i=0; i<str.length(); i++)   {    int charCode = str.codePointAt(i);    if(charCode >= 65 && charCode<=90)     count[charCode-65]++;   }      for(int i=0; i<alphabets.length; i++)   {    System.out.print(alphabets[i] + ": ");    for(int j=0; j<count[i]; j++)     System.out.print("=");    System.out.print("\n");   }  } }
