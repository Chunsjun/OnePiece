abstract class Unit {

 int x, y;

 abstract void move(int x, int y); // 세미콜론으로 끝난다는 것에 유의

void stop() { /* 현재 위치에 정지 */}

 }



class Marine extends Unit {

 int x, y;

 void move(int x, int y) { /* 지정된 위치로 이동 */ }

void stimpack()

 }



 class Tank extends Unit {

 int x, y;

 void move(int x, int y) { /* 지정된 위치로 이동 */ }

void SiegeMode()

 }



 class Dropship extends Unit {

 int x, y;

 void move(int x, int y) { /* 지정된 위치로 이동 */ }

void unload()

 }
