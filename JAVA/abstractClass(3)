class CDPlayer extends Player { 
     // 조상 클래스의 추상메서드를 구현한다. 
      void play(int currentPos) { 
            /* 실제구현 내용 생략 */ 
      } 

      void stop() { 
            /* 실제구현 내용 생략 */ 
      } 

      // CDPlayer클래스에 추가로 정의된 멤버 
      int currentTrack; // 현재 재생 중인 트랙 

      void nextTrack() { 
            currentTrack++; 
            //... 
      } 

      void preTrack() { 
            if(currentTrack > 0) { 
                  currentTrack--; 
            } 
            //... 
      } 
} 
